CREATE DATABASE IF NOT EXISTS LL_data CHARACTER SET=utf8;
USE LL_data;
CREATE TABLE IF NOT EXISTS LL_bibi_ios(
	id INT NOT NULL auto_increment,

	date DATE NOT NULL PRIMARY KEY,

	ios_user_count INT NOT NULL,	

	average_online_ios FLOAT NOT NULL,
	online_fractile_ios_5 FLOAT NOT NULL,
	online_fractile_ios_20 FLOAT NOT NULL,
	online_fractile_ios_50 FLOAT NOT NULL,
	online_fractile_ios_80 FLOAT NOT NULL,
	online_fractile_ios_95 FLOAT NOT NULL,

	ios_online_section_4h FLOAT NOT NULL,
	ios_online_section_8h FLOAT NOT NULL,
	ios_online_section_12h FLOAT NOT NULL,
	ios_online_section_16h FLOAT NOT NULL,
	ios_online_section_20h FLOAT NOT NULL,
	ios_online_section_24h FLOAT NOT NULL,

	avg_databyte_ios INT NOT NULL,
	data_byte_fractile_ios_50 INT NOT NULL,
	data_byte_fractile_ios_80 INT NOT NULL,
	data_byte_fractile_ios_95 INT NOT NULL,

	data_byte_section_ios_100k FLOAT NOT NULL,
	data_byte_section_ios_1m FLOAT NOT NULL,
	data_byte_section_ios_10m FLOAT NOT NULL,
	data_byte_section_ios_100m FLOAT NOT NULL,

	median_timeused_ios FLOAT NOT NULL,

	avg_channel_time_ios INT NOT NULL,

	avg_carrier_match_ios FLOAT NOT NULL,

	ios_history_match FLOAT NOT NULL,
);
CREATE TABLE IF NOT EXISTS LL_bibi_android(
