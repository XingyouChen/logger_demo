	online_fractile_android_95 FLOAT NOT NULL,

	android_online_section_4h FLOAT NOT NULL,
	android_online_section_8h FLOAT NOT NULL,
	android_online_section_12h FLOAT NOT NULL,
	android_online_section_16h FLOAT NOT NULL,
	android_online_section_20h FLOAT NOT NULL,
	android_online_section_24h FLOAT NOT NULL,

	avg_databyte_android INT NOT NULL,
	data_byte_fractile_android_50 INT NOT NULL,
	data_byte_fractile_android_80 INT NOT NULL,
	data_byte_fractile_android_95 INT NOT NULL,

	data_byte_section_android_100k FLOAT NOT NULL,
	data_byte_section_android_1m FLOAT NOT NULL,
	data_byte_section_android_10m FLOAT NOT NULL,
	data_byte_section_android_100m FLOAT NOT NULL,

	median_timeused_android FLOAT NOT NULL,

	avg_channel_time_android INT NOT NULL,

	avg_carrier_match_android FLOAT NOT NULL,

	android_history_match FLOAT NOT NULL,
);

